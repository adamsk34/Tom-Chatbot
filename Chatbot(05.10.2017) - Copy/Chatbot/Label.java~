
public class Label
{
  private ArrayList<String> words = new ArrayList<String>() ;
  
  public Label (ArrayList<String> wordsGiven)
  {
    words = wordsGiven ;
  }
  public boolean equals(ArrayList<String> wordsGiven)
  {
    boolean result = true ;
    
    if(words.size() != wordsGiven.size())
      result = false ;
    else
    {
      for(int i = 0 ; i < words.size() && result ; i++)
      {
        if(words.get(i).equals(  wordsGiven.get(i)  ))
          result = false ;
      }
    }
    
    return result ;
  }
}
